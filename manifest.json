{
  "manifest_version": 2,

  "name": "HNews Tab",
  "description": "This extension shows a hacker news articles on your new tab page",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab": "hnews.html"
  },
  "icons" : {
    "16":  "images/logo16.png",
    "32":  "images/logo32.png",
    "48":  "images/logo48.png",
    "128": "images/logo128.png"
  },
  "permissions": [
    "storage"
  ]
}
